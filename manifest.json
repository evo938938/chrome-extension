{
  "manifest_version": 2,
  "name": "Play With Docker",
  "version": "0.0.1",
  "content_scripts": [{
    "matches": ["https://hub.docker.com/_/*/", "https://hub.docker.com/r/*/*/"],
    "css": ["vendor/bootstrap.min.css", "assets/css/pwd.css"],
    "js": ["vendor/jquery-3.2.1.min.js", "vendor/bootstrap.min.js", "assets/js/pwd.js"]
  }],
  "browser_action": {
    "default_title": "Play With Docker"
  },
  "permissions": ["activeTab"],
  "web_accessible_resources": [
      "assets/images/*"
  ]
}
